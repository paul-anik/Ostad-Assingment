@model IEnumerable<QnAApp.Models.Question>

<h2>Questions</h2>

<ul>
    @foreach (var question in Model)
    {
        <li>
            <a href="@Url.Action("Details", "Questions", new { id = question.Id })">@question.Title</a>
        </li>
    }
</ul>

<a href="@Url.Action("Create", "Questions")" class="btn btn-primary">Ask a Question</a>
